<h1>Listing expenses</h1>

<table>
  <thead>
    <tr>
      <th>Period end date</th>
      <th>Habby</th>
      <th>Bank charge</th>
      <th>Equipment</th>
      <th>Stationery</th>
      <th>Computer</th>
      <th>Telephone</th>
      <th>Misc expense</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.period_end_date %></td>
        <td><%= expense.habby %></td>
        <td><%= expense.bank_charge %></td>
        <td><%= expense.equipment %></td>
        <td><%= expense.stationery %></td>
        <td><%= expense.computer %></td>
        <td><%= expense.telephone %></td>
        <td><%= expense.misc_expense %></td>
        <td><%= link_to 'Show', expense %></td>
        <td><%= link_to 'Edit', edit_expense_path(expense) %></td>
        <td><%= link_to 'Destroy', expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense', new_expense_path %>
